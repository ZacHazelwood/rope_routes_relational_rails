<%= form_with url: '/gyms', method: :post, local: true do |form| %>
  <%= form.label :name %><br>
  <%= form.text_field :name %><br>
  <%= form.label :location %><br>
  <%= form.text_field :location %><br>
  <%= form.label "Has Rope" %><br>
  <%= form.select :has_rope, [true, false] %><br>
  <%= form.label "Square Feet" %><br>
  <%= form.text_field :square_feet %><br>
  <%= form.submit "Create Gym" %>
<% end %>
